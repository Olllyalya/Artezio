import gc
def any_numbers_of_argument(*args): #все, что получилочь сделать, знаю, что не работает)
    multiplication = 1
    for i in args:
        if i != 0:
            multiplication = i*multiplication
    print(multiplication)
    summ = 0
    for i in args:
        if i != 0:
            summ += i
    print(summ)
    
    
